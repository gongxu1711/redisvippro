package itforstudent;

public class BT1 {
	static void myMethod() {
		System.out.println("1234");
	}
	
	static void showName(String fname) {
		System.out.println(fname + "Nguyen");
	}
	
	static void showAge(String firstName, int age) {
		System.out.println(firstName + " is " + age);
	}
	
	public static void main(String[] args) {
		for(int i = 0; i < 10; i++) {
			if(i == 4) {
				continue;
			} else {
				System.out.println(i);
			}
		}
		int arr[] = {1,2,3,4,5,5,6,6,7};
		String materials[] = {"Poly","Cotton","Tici","Lua","Theu"};
		for(int x : arr) {
			System.out.println(x);
		}
		String[] cars = {"Volvo", "BMW", "Ford", "Mazda"};
		for (int i = 0; i < cars.length; i++) {
		  System.out.println(cars[i]);
		  System.out.println(cars.length);
		}
		int[][] myNum = {{1,2,3,4,5},{2,3,4,5}};
		System.out.println(myNum[1][3]);
		myMethod();
		showName("Tu");
		showName("Dung");
		showName("Hung");
		showAge("Dung", 1);
	}
}

